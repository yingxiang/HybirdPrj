{
    "UIContainerType": "UIContainerWindow",
    "identify": "statusWindow",
    "windowLevel": "1",
  "hidden": true,
    "cornerRadius": "4",
    "clipsToBounds": true,
    "subViews": [
                 {
                 "UIContainerType": "UIContainerView",
                 "identify": "toolbar",
                 "backgroundColor": "#333333e0",
      "subViews": [
        {
          "UIContainerType": "UIContainerLabel",
          "identify": "label",
          "backgroundColor": "#00000000",
          "textColor": "#ffffff",
          "font": "13",
          "textAlignment": "1",
          "numberOfLines": "0",
          "text": "状态栏message",
          "frame": {
            "x": "0",
            "y": "0",
            "width": "160",
            "height": "32"
          },
          "autoSizefit":true
        }
      ],
      "frame": {
                 "x":  "0"
                 ,
                 "y":  "5"
                 ,
                 "width":  "160"
                 ,
                 "height": "+(self.container_toolbar.container_label.view.height)(10)"
                 
                 }
    }
                 ],
    "functionList": [
                     {
                     "functionname": "UIControlEventTouchUpInside",
                     "type": "FT_UPDATEVIEW",
                     "alpha": "1",
                     "hidden": false
                     },
                     {
                     "functionname": "UIControlEventTouchUpInside_complete",
                     "type": "FT_UPDATEVIEW",
                     "alpha": "0",
                     "hidden": false,
                     "animation": {
                     "duration": "0.5"
                     }
                     },
                     {
                     "functionname": "UIControlEventTouchUpInside_complete_block",
                     "type": "FT_UPDATEVIEW",
                     "hidden": true
                     },
                     {
                     "functionname": "show",
                     "type": "FT_UPDATEVIEW",
                     "alpha": "0",
                     "hidden": false,
                     "subViews": [
                                  {
                                  "identify": "toolbar",
                                  "subViews": [
                                               {
                                               "identify": "label",
                                               "text": "parmer~parmer1"
                                               }
                                               ]
                                  }
                                  ]
                     },
                     {
                     "functionname": "show_complete",
                     "type": "FT_UPDATEVIEW",
                     "alpha": "1",
                     "animation": {
                     "duration": "0.5"
                     }
                     },
                     {
                     "functionname": "show_complete_block",
                     "type": "FT_UPDATEVIEW",
                     "delay": "1.0",
                     "alpha": "0",
                     "animation": {
                     "duration": "0.5"
                     }
                     },
                     {
                     "functionname": "show_complete_block_block",
                     "type": "FT_UPDATEVIEW",
                     "hidden": true
                     }
                     ],
  "frame": {
    "x": "/-(self.windowContainer.view.width)(160)(2)",
    "y": "-(self.windowContainer.view.height)(self.view.height)",
    "width": "160",
    "height": "+(self.container_toolbar.container_label.view.height)(10)"
  }
}